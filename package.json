{
     "name": "is.t",
  "version": "0.0.0",

  "description": "runtime typecheck library",

  "license": "ISC",
   "author": "Strider <strange.transistor@gmail.com>",

     "type": "module",
  "engines": { "node": ">= 18" },

  "exports":
  {
    "./type/*": "./type/*.js",

    "./Wrong":  "./Wrong.js",
    "./check":  "./check.js",
    "./match":  "./match.js",
    "./handle": "./handle.js",

    "./_/assess":  "./_/assess.js",
    "./_/attempt": "./_/attempt.js"
  },

  "files":
  [
    "*.js",
    "type/",
    "_/"
  ],

  "scripts":
  {
         "st": "eslint",
        "try": "node -r console-ultimate example/mixed.js",

       "test": "run-s st try",

    "example": "cd example; node -r console-ultimate"
  },

  "dependencies":
  {
    "function-name":
      "2",

    "def-prop":
      "2"
  },

  "devDependencies":
  {
    "is.t":
      "./",

    "npm-run-all":
      "4.1.5",

    "console-ultimate":
      "3",

    "eslint":
      "9.38",

    "outlander":
      "StrangeTransistor/outlander#3.0.0",

    "eslint-plugin-node":
      "11"
  }
}
